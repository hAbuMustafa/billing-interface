{
  "name": "billing-interface",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "db:seed": "node scripts/seed/",
    "db:push": "pnpm drizzle-kit push --config=drizzle-dev.config.ts",
    "db:generate": "pnpm drizzle-kit generate --config=drizzle-dev.config.ts"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^24.0.15",
    "drizzle-kit": "^0.31.4",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^7.0.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "dependencies": {
    "@libsql/client": "^0.15.15",
    "bcryptjs": "^3.0.2",
    "dayjs": "^1.11.13",
    "drizzle-orm": "^0.44.5",
    "extend-arabic-query": "^0.3.2",
    "lodash.debounce": "^4.0.8"
  }
}
